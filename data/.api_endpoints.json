      "path": "/api/trading/cancel",
      "method": "POST",
      "params": {"order_id": "id to cancel", "cancel_all": "true to cancel all"},
      "description": "Cancel order(s)"
    },
    "trading_mass_order": {
      "path": "/api/trading/mass-order",
      "method": "POST",
      "params": {"orders": "[{market_id, side, outcome, price, size}, ...]", "delay_ms": "delay between orders (default 100)"},
      "description": "Place multiple orders at once"
    },
    "trading_bracket": {
      "path": "/api/trading/bracket",
      "method": "GET",
      "params": {"id": "market_id", "center": "center price 0-1", "spread": "spread from center", "size": "order size", "outcome": "yes/no"},
      "description": "Place bracket orders (buy below + sell above center)"
    },
