{
  "version": "1.0",
  "updated": "2026-01-20",
  "endpoints": {
    "market": {
      "path": "/api/market",
      "method": "GET",
      "params": {"id": "market_id (required)"},
      "description": "Get market details by ID"
    },
    "orderbook": {
      "path": "/api/orderbook",
      "method": "GET",
      "params": {"id": "market_id (required)"},
      "description": "Get orderbook (bids/asks) for a market"
    },
    "trades": {
      "path": "/api/trades",
      "method": "GET",
      "params": {"id": "market_id (required)"},
      "description": "Get recent trades for a market"
    },
    "search": {
      "path": "/api/search",
      "method": "GET",
      "params": {"q": "query string", "limit": "max results (default 20)"},
      "description": "Search markets via Polyrouter"
    },
    "watch": {
      "path": "/api/watch",
      "method": "GET",
      "params": {"id": "market_id", "title": "market title"},
      "description": "Add market to watchlist"
    },
    "unwatch": {
      "path": "/api/unwatch",
      "method": "GET",
      "params": {"id": "market_id"},
      "description": "Remove market from watchlist"
    },
    "watched": {
      "path": "/api/watched",
      "method": "GET",
      "params": {},
      "description": "Get all watched markets"
    },
    "history": {
      "path": "/api/history",
      "method": "GET",
      "params": {"id": "market_id"},
      "description": "Get price history for market"
    },
    "cache_size": {
      "path": "/api/cache-size",
      "method": "GET",
