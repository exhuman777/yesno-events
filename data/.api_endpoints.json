    },
    "search_similar": {
      "path": "/api/search/similar",
      "method": "GET",
      "params": {"id": "market_id", "limit": "max results"},
      "description": "Find similar markets"
    },
    "kelly": {
      "path": "/api/kelly",
      "method": "GET",
      "params": {"prob": "your probability", "price": "market price", "bankroll": "your bankroll", "fraction": "kelly fraction"},
      "description": "Kelly criterion calculator"
    },
