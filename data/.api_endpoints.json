      "path": "/api/trading/price",
      "method": "GET",
      "params": {"id": "market_id"},
      "description": "Get current YES/NO prices"
    },
    "trading_book": {
      "path": "/api/trading/book",
      "method": "GET",
      "params": {"id": "market_id", "outcome": "yes/no"},
      "description": "Get orderbook with sizes"
    },
    "trading_orders": {
      "path": "/api/trading/orders",
      "method": "GET",
      "params": {},
      "description": "Get your open orders"
    },
    "trading_positions": {
      "path": "/api/trading/positions",
      "method": "GET",
      "params": {},
      "description": "Get your positions"
    },
    "trading_trades": {
      "path": "/api/trading/trades",
      "method": "GET",
      "params": {"limit": "max trades (default 50)"},
      "description": "Get your trade history"
    },
    "trading_order": {
      "path": "/api/trading/order",
      "method": "POST",
      "params": {"market_id": "id", "side": "BUY/SELL", "outcome": "yes/no", "price": "0-1", "size": "quantity"},
      "description": "Place a single order"
    },
