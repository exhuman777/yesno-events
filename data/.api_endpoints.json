      "path": "/api/cached-trades",
      "method": "GET",
      "params": {"id": "market_id"},
      "description": "Get cached trades for market"
    },
    "price_history": {
      "path": "/api/price-history",
      "method": "GET",
      "params": {"id": "market_id", "interval": "1h/1d/1w"},
      "description": "Get price history with interval"
    },
    "diary": {
      "path": "/api/diary",
      "method": "GET",
      "params": {},
      "description": "Get all trading diary entries"
    },
    "diary_date": {
      "path": "/api/diary/date",
      "method": "GET",
      "params": {"date": "YYYY-MM-DD"},
