      "path": "/api/elon/current",
      "method": "GET",
      "params": {},
      "description": "Get current Elon tweet count"
    },
    "elon_tweets": {
      "path": "/api/elon/tweets",
      "method": "GET",
      "params": {},
      "description": "Get Elon tweet history"
