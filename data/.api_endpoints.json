      "path": "/api/alerts",
      "method": "GET",
      "params": {},
      "description": "Get all price alerts"
    },
    "alerts_active": {
      "path": "/api/alerts/active",
      "method": "GET",
      "params": {},
      "description": "Get active (non-triggered) alerts"
    },
